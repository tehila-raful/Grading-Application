<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gradingapp.FXMLAverageByClassController">
   <children>
      <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0">
         <children>
            <Label text="Enter date of exam: (yyyy-mm-dd)">
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <TextField fx:id="examDateString" maxWidth="-Infinity" prefHeight="31.0" prefWidth="269.0" promptText="Date ">
               <VBox.margin>
                  <Insets bottom="20.0" left="5.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="btnDate" mnemonicParsing="false" onAction="#SubmitDate" text="Submit Date ">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="lblAverage">
               <VBox.margin>
                  <Insets left="70.0" top="30.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <Button fx:id="btnBack" layoutX="23.0" layoutY="355.0" mnemonicParsing="false" onAction="#btnBackClicked" text="Back" />
   </children>
</AnchorPane>
